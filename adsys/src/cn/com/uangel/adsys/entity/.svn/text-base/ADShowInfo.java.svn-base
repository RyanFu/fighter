package cn.com.uangel.adsys.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 展示记录
 * 
 * @author Tree
 * 
 */
public class ADShowInfo implements Serializable {

	private static final long serialVersionUID = 1L;

	private Integer id;
	private String app_id;
	private Integer ad_id;
	private Date show_time;
	private String imei;
	private Double cur_cpm;
	private String zone;
	private Integer show_type_id;
	/** 此次cpm收入给开发者的分红：如cur_cpm*80% */
	private Double cpm_income;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getApp_id() {
		return app_id;
	}

	public void setApp_id(String app_id) {
		this.app_id = app_id;
	}

	public Integer getAd_id() {
		return ad_id;
	}

	public void setAd_id(Integer ad_id) {
		this.ad_id = ad_id;
	}

	public Date getShow_time() {
		return show_time;
	}

	public void setShow_time(Date show_time) {
		this.show_time = show_time;
	}

	public String getImei() {
		return imei;
	}

	public void setImei(String imei) {
		this.imei = imei;
	}

	public Double getCur_cpm() {
		return cur_cpm;
	}

	public void setCur_cpm(Double cur_cpm) {
		this.cur_cpm = cur_cpm;
	}

	public String getZone() {
		return zone;
	}

	public void setZone(String zone) {
		this.zone = zone;
	}

	public Integer getShow_type_id() {
		return show_type_id;
	}

	public void setShow_type_id(Integer show_type_id) {
		this.show_type_id = show_type_id;
	}

	public Double getCpm_income() {
		return cpm_income;
	}

	public void setCpm_income(Double cpm_income) {
		this.cpm_income = cpm_income;
	}

}
