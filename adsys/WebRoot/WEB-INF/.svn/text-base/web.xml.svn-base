	<filter-mapping>
	 	<filter-name>AuthenticationFilter</filter-name>
	    <url-pattern>/ctrl/saveAdBasicInfo.ad</url-pattern>  
	</filter-mapping>
	<filter-mapping>
	 	<filter-name>AuthenticationFilter</filter-name>
	    <url-pattern>/ctrl/saveAdBasicInfo.ad</url-pattern>  
