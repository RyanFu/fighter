	<!-- 王健 -->
	
	<!-- 基本信息 -->
	<filter-mapping>
	 	<filter-name>AuthenticationFilter</filter-name>
	    <url-pattern>/jsp/member/accountInformation.jsp</url-pattern>  
	</filter-mapping>
	<filter-mapping>
	 	<filter-name>AuthenticationFilter</filter-name>
	    <url-pattern>/jsp/member/financialInformation.jsp</url-pattern>  
