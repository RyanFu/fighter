package cn.com.uangel.adsdk.entity;

import android.graphics.drawable.Drawable;
import android.view.View;

/**
 * 点击显示方式 1、浏览网页2、下载应用3、打电话 4、发信息 5、发邮件 6、地图 7、视屏 8、音乐 9、显示全屏图片
 */

public class Advertisement {

	private String showTypeID;
	private String adID;
	private int click_result;// 广告点击后显示方式
	private String detail;// 广告详细描述信息
	private String tel;// 电话号
	private String sms;// 信息内容
	private String[] email;// 邮箱地址
	private String email_title;// 邮件标题
	private String email_content;// 邮件内容
	private Drawable images;// 广告图片（logo或者整张条幅图片）
	private Drawable clickFullImg;// 点击后的全屏图片
	private String adString;// 广告语
	private String adTitle;// 广告标题
	private String video_url;// 视频文件地址
	private String mp3_url;// 音频文件地址
	private String web_url;// 网页及其下载地址
	private double lat;// 经度 116.33132
	private double lon;// 纬度 39.70432
	private int titleOrImg; // 0：显示文字＋图片，1： 仅显示图片，2：全屏图片，3：视频动画
	private View clickWhichView;

	public String getShowTypeID() {
		return showTypeID;
	}

	public void setShowTypeID(String showTypeID) {
		this.showTypeID = showTypeID;
	}

	public String getAdID() {
		return adID;
	}

	public void setAdID(String adID) {
		this.adID = adID;
	}

	public int getClick_result() {
		return click_result;
	}

	public void setClick_result(int click_result) {
		this.click_result = click_result;
	}

	public String getDetail() {
		return detail;
	}

	public void setDetail(String detail) {
		this.detail = detail;
	}

	public String getTel() {
		return tel;
	}

	public void setTel(String tel) {
		this.tel = tel;
	}

	public String getSms() {
		return sms;
	}

	public void setSms(String sms) {
		this.sms = sms;
	}

	public String[] getEmail() {
		return email;
	}

	public void setEmail(String[] email) {
		this.email = email;
	}

	public String getEmail_title() {
		return email_title;
	}

	public void setEmail_title(String email_title) {
		this.email_title = email_title;
	}

	public String getEmail_content() {
		return email_content;
	}

	public void setEmail_content(String email_content) {
		this.email_content = email_content;
	}

	public Drawable getImages() {
		return images;
	}

	public void setImages(Drawable images) {
		this.images = images;
	}

	public String getAdString() {
		return adString;
	}

	public void setAdString(String adString) {
		this.adString = adString;
	}

	public String getAdTitle() {
		return adTitle;
	}

	public void setAdTitle(String adTitle) {
		this.adTitle = adTitle;
	}

	public String getVideo_url() {
		return video_url;
	}

	public void setVideo_url(String video_url) {
		this.video_url = video_url;
	}

	public String getMp3_url() {
		return mp3_url;
	}

	public void setMp3_url(String mp3_url) {
		this.mp3_url = mp3_url;
	}

	public String getWeb_url() {
		return web_url;
	}

	public void setWeb_url(String web_url) {
		this.web_url = web_url;
	}

	public double getLat() {
		return lat;
	}

	public void setLat(double lat) {
		this.lat = lat;
	}

	public double getLon() {
		return lon;
	}

	public void setLon(double lon) {
		this.lon = lon;
	}

	public View getClickWhichView() {
		return clickWhichView;
	}

	public void setClickWhichView(View clickWhichView) {
		this.clickWhichView = clickWhichView;
	}

	public int getTitleOrImg() {
		return titleOrImg;
	}

	public void setTitleOrImg(int titleOrImg) {
		this.titleOrImg = titleOrImg;
	}

	public Drawable getClickFullImg() {
		return clickFullImg;
	}

	public void setClickFullImg(Drawable clickFullImg) {
		this.clickFullImg = clickFullImg;
	}

}
